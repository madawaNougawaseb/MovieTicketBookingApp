      <console name="MovieBaninkingApp" type="STANDARD" schema="mysql" session="Main"><![CDATA[package gui;

import java.awt.*;
import java.awt.event.*;
import javax.swing.*;
import javax.swing.border.EmptyBorder;
import java.util.ArrayList;
import java.util.List;

public class SeatSelectionFrame extends JFrame {

    // Constants
    private static final int ROWS = 8;
    private static final int COLS = 10;
    private static final Color AVAILABLE_COLOR = new Color(144, 238, 144); // Light green
    private static final Color SELECTED_COLOR = new Color(100, 149, 237);  // Cornflower blue
    private static final Color BOOKED_COLOR = new Color(211, 211, 211);    // Light gray

    // Components
    private JPanel mainPanel, seatPanel, infoPanel, controlPanel;
    private JButton[][] seats;
    private JButton btnConfirm, btnCancel, btnToBookingFrame;
    private JLabel lblScreen, lblMovieInfo, lblTimeInfo, lblSelectedSeats, lblTotalPrice;

    // Data
    private boolean[][] bookedSeats;
    private List<String> selectedSeatsList;
    private double ticketPrice = 12.50;  // Default price
    private String movieName = "Avengers: Endgame";
    private String theaterName = "Cinema 3";
    private String showTime = "May 8, 2025 - 7:30 PM";

    public SeatSelectionFrame() {
        initializeData();
        createComponents();
        layoutComponents();
        addEventHandlers();

        setTitle("Seat Selection - " + movieName);
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        setSize(900, 700);
        setLocationRelativeTo(null);
    }

    private void initializeData() {
        bookedSeats = new boolean[ROWS][COLS];
        selectedSeatsList = new ArrayList<>();
        
        // Simulate some pre-booked seats (for testing only)
        bookedSeats[1][2] = true;
        bookedSeats[1][3] = true;
        bookedSeats[4][5] = true;
        bookedSeats[6][7] = true;
        bookedSeats[6][8] = true;
    }

    private void createComponents() {
        // Initialize panels
        mainPanel = new JPanel(new BorderLayout(10, 10));
        mainPanel.setBorder(new EmptyBorder(20, 20, 20, 20));
        mainPanel.setBackground(new Color(30, 30, 30));
        
        seatPanel = new JPanel(new GridLayout(ROWS, COLS, 5, 5));
        seatPanel.setBackground(new Color(30, 30, 30));
        
        infoPanel = new JPanel();
        infoPanel.setLayout(new BoxLayout(infoPanel, BoxLayout.Y_AXIS));
        infoPanel.setBackground(new Color(40, 40, 40));
        infoPanel.setBorder(BorderFactory.createEmptyBorder(15, 15, 15, 15));
        
        controlPanel = new JPanel();
        controlPanel.setLayout(new FlowLayout(FlowLayout.CENTER, 20, 10));
        controlPanel.setBackground(new Color(30, 30, 30));
        
        // Create screen label
        lblScreen = new JLabel("SCREEN", SwingConstants.CENTER);
        lblScreen.setPreferredSize(new Dimension(600, 30));
        lblScreen.setBackground(new Color(70, 70, 70));
        lblScreen.setForeground(Color.WHITE);
        lblScreen.setOpaque(true);
        
        // Create info labels
        lblMovieInfo = new JLabel(movieName + " - " + theaterName);
        lblMovieInfo.setForeground(Color.WHITE);
        lblMovieInfo.setAlignmentX(Component.CENTER_ALIGNMENT);
        
        lblTimeInfo = new JLabel(showTime);
        lblTimeInfo.setForeground(Color.WHITE);
        lblTimeInfo.setAlignmentX(Component.CENTER_ALIGNMENT);
        
        lblSelectedSeats = new JLabel("Selected Seats: None");
        lblSelectedSeats.setForeground(Color.WHITE);
        lblSelectedSeats.setAlignmentX(Component.CENTER_ALIGNMENT);
        
        lblTotalPrice = new JLabel("Total Price: $0.00");
        lblTotalPrice.setForeground(Color.WHITE);
        lblTotalPrice.setAlignmentX(Component.CENTER_ALIGNMENT);
        
        // Create buttons
        btnConfirm = new JButton("Confirm Selection");
        btnCancel = new JButton("Cancel Selection");
        btnToBookingFrame = new JButton("Proceed to Payment");
        btnToBookingFrame.setEnabled(false);
        
        // Create seat buttons
        seats = new JButton[ROWS][COLS];
        for (int i = 0; i < ROWS; i++) {
            for (int j = 0; j < COLS; j++) {
                seats[i][j] = new JButton(String.format("%c%d", (char)('A' + i), j + 1));
                seats[i][j].setPreferredSize(new Dimension(50, 50));
                seats[i][j].setMargin(new Insets(0, 0, 0, 0));
                
                if (bookedSeats[i][j]) {
                    seats[i][j].setBackground(BOOKED_COLOR);
                    seats[i][j].setEnabled(false);
                } else {
                    seats[i][j].setBackground(AVAILABLE_COLOR);
                }
                
                final int row = i;
                final int col = j;
                seats[i][j].addActionListener(e -> toggleSeatSelection(row, col));
                
                seatPanel.add(seats[i][j]);
            }
        }
    }

    private void layoutComponents() {
        // Add components to info panel
        infoPanel.add(lblMovieInfo);
        infoPanel.add(Box.createRigidArea(new Dimension(0, 10)));
        infoPanel.add(lblTimeInfo);
        infoPanel.add(Box.createRigidArea(new Dimension(0, 20)));
        infoPanel.add(lblSelectedSeats);
        infoPanel.add(Box.createRigidArea(new Dimension(0, 10)));
        infoPanel.add(lblTotalPrice);
        
        // Add components to control panel
        controlPanel.add(btnConfirm);
        controlPanel.add(btnCancel);
        controlPanel.add(btnToBookingFrame);
        
        // Combine panels
        JPanel centerPanel = new JPanel(new BorderLayout(10, 20));
        centerPanel.setBackground(new Color(30, 30, 30));
        centerPanel.add(lblScreen, BorderLayout.NORTH);
        centerPanel.add(seatPanel, BorderLayout.CENTER);
        
        mainPanel.add(centerPanel, BorderLayout.CENTER);
        mainPanel.add(infoPanel, BorderLayout.EAST);
        mainPanel.add(controlPanel, BorderLayout.SOUTH);
        
        // Set content pane
        setContentPane(mainPanel);
    }

    private void addEventHandlers() {
        btnConfirm.addActionListener(e -> {
            if (!selectedSeatsList.isEmpty()) {
                // In a real application, save the booking to a database
                JOptionPane.showMessageDialog(this, 
                    "Booking confirmed for " + selectedSeatsList.size() + " seats.\n" +
                    "Total: $" + String.format("%.2f", selectedSeatsList.size() * ticketPrice),
                    "Booking Confirmed", JOptionPane.INFORMATION_MESSAGE);
                btnToBookingFrame.setEnabled(true);
            } else {
                JOptionPane.showMessageDialog(this, 
                    "Please select at least one seat.", 
                    "No Seats Selected", JOptionPane.WARNING_MESSAGE);
            }
        });
        
        btnCancel.addActionListener(e -> cancelBooking());
        
        // IMPORTANT FIX: Create a new instance of BookingFrame here
        btnToBookingFrame.addActionListener(e -> {
            // Create a fresh instance of the BookingFrame
            BookingFrame bookingFrame = new BookingFrame();
            bookingFrame.setVisible(true);
            dispose(); // Close this frame
        });
    }

    private void toggleSeatSelection(int row, int col) {
        JButton seat = seats[row][col];
        String seatLabel = String.format("%c%d", (char)('A' + row), col + 1);
        
        if (seat.getBackground().equals(AVAILABLE_COLOR)) {
            seat.setBackground(SELECTED_COLOR);
            selectedSeatsList.add(seatLabel);
        } else {
            seat.setBackground(AVAILABLE_COLOR);
            selectedSeatsList.remove(seatLabel);
        }
        
        updateSelectionInfo();
    }

    private void updateSelectionInfo() {
        if (selectedSeatsList.isEmpty()) {
            lblSelectedSeats.setText("Selected Seats: None");
            lblTotalPrice.setText("Total Price: $0.00");
        } else {
            StringBuilder sb = new StringBuilder("Selected Seats: ");
            for (int i = 0; i < selectedSeatsList.size(); i++) {
                sb.append(selectedSeatsList.get(i));
                if (i < selectedSeatsList.size() - 1) {
                    sb.append(", ");
                }
            }
            lblSelectedSeats.setText(sb.toString());
            
            double total = selectedSeatsList.size() * ticketPrice;
            lblTotalPrice.setText(String.format("Total Price: $%.2f", total));
        }
    }

    private void cancelBooking() {
        // Reset all selected seats to available
        for (int i = 0; i < ROWS; i++) {
            for (int j = 0; j < COLS; j++) {
                if (!bookedSeats[i][j] && !seats[i][j].getBackground().equals(AVAILABLE_COLOR)) {
                    seats[i][j].setBackground(AVAILABLE_COLOR);
                }
            }
        }
        
        selectedSeatsList.clear();
        updateSelectionInfo();
        btnToBookingFrame.setEnabled(false);
    }

    public static void main(String[] args) {
        SwingUtilities.invokeLater(() -> {
            new SeatSelectionFrame().setVisible(true);
        });
    }
}]]></console>
